<div class="page">
<h1>New news</h1>
<% form_for @news, :url => {:action => :create} do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit 'Create', :class => "submit" %>
  </p>
<% end %>
</div>
</div>
<div id="body_wrapper">
</div>

