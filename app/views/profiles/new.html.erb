<h1>New profile</h1>

<% form_for(@profile) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :interests %><br />
    <%= f.text_area :interests %>
  </p>
  <p>
    <%= f.label :qualifications %><br />
    <%= f.text_area :qualifications %>
  </p>
  <p>
    <%= f.label :avatar_url %><br />
    <%= f.text_field :avatar_url %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', profiles_path %>
